#!/usr/bin/env python3.6
import sys
x, y = sys.argv[1:3]
with open(x) as xs:
    xs = (x.split('\t') for x in xs)
    xs = {path: (path, *more) for path, *more in xs}
with open(y) as ys:
    ys = (y.split('\t') for y in ys)
    for path, *more in ys:
        if path in xs:
            print('\t'.join(xs[path]).strip())
        else:
            print('\t'.join([path, *more]).strip())
