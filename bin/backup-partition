#!/usr/bin/env python3.6
import sys
files = {}
for line in sys.stdin:
    try:
        path, tar, _blake2b, _size = line.strip().split('\t')
    except:
        print('error:', line)
        raise
    if not tar.startswith('ref::'):
        if tar not in files:
            files[tar] = open(tar, 'w')
        try:
            files[tar].write(path + '\n')
        except:
            print('error:', line)
            raise
for k in sorted(files):
    print(k)
    files[k].close()
